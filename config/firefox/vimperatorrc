" General {{{1
" autocomplete
set complete=lsb

" longest matching
set wildmode='list:full'

" no beeping
set noerrorbells

" startpage as default search engine
set defsearch=startpage
" }}}1

" Appearance {{{1
colorscheme molokai
set gui=none,tabs,navigation
" }}}1

" Key mappings {{{1
let mapleader=","

nnoremap <leader>q :q<cr>
" }}}1

