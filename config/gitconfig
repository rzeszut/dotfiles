[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    lf = ls-files
    st = status
    fe = fetch
    re = rebase
    me = merge
    ci = commit
    co = checkout
    br = branch

    sf = svn fetch
    sr = svn rebase
    sp = svn dcommit

[push]
	default = simple
