" General {{{1
" autocomplete
set complete=lsb

" longest matching
set wildmode='list:full'

" no beeping
set noerrorbells

" duckduckgo as default search engine
set defsearch=startpage

" use JIT
set! javascript.option.jit.chrome=true
" }}}1

" Appearance {{{1
colorscheme molokai
set gui=none,tabs,navigation
" }}}1

" Key mappings {{{1
let mapleader=","

nnoremap <leader>q :q<cr>
" }}}1

